        android:layout_toStartOf="@+id/tv_inicadasItemClave"
        android:textColor="@color/colorPrimaryDark"/>

    <TextView
        android:text="Termino"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView29"
        android:gravity="center"
        android:layout_alignBaseline="@+id/textView28"
        android:layout_alignBottom="@+id/textView28"
        android:layout_centerHorizontal="true"
        android:textColor="@color/colorPrimaryDark"/>

    <TextView
        android:text="Cantidad"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView27"
        android:gravity="center"
        android:layout_alignBaseline="@+id/textView29"
        android:layout_alignBottom="@+id/textView29"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="20dp"
        android:textColor="@color/colorPrimaryDark"/>

    <TextView
        android:text="00:00 AM"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView28"
        android:layout_alignRight="@+id/textView28"
        android:layout_alignEnd="@+id/textView28"
        android:layout_marginRight="16dp"
        android:layout_marginEnd="16dp"
        android:id="@+id/tv_iniciadasItemInicio"
        android:textColor="@color/colorPrimary"/>

    <TextView
        android:text="00:00 Am"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView29"
        android:layout_centerHorizontal="true"
        android:id="@+id/tv_iniciadasItemTermino"
        android:textColor="@color/colorPrimary"/>

    <TextView
        android:text="0.0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView27"
        android:id="@+id/tv_inicadasItemCantidad"
        android:layout_alignRight="@+id/textView27"
        android:layout_alignEnd="@+id/textView27"
        android:layout_alignLeft="@+id/textView27"
        android:layout_alignStart="@+id/textView27"
        android:gravity="center"
        android:textColor="@color/colorPrimary"/>

    <TextView
        android:text="Observaciones"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv_iniciadasItemInicio"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/textView33"
        android:layout_marginTop="5dp"
        android:textColor="@color/colorPrimaryDark"/>

    <TextView
        android:text="TextView"
        android:layout_width="265dp"
        android:layout_height="wrap_content"
        android:id="@+id/tv_iniciadasItemTipo"
        android:layout_alignBaseline="@+id/textView25"
        android:layout_alignBottom="@+id/textView25"
        android:layout_toLeftOf="@+id/textView25"
        android:layout_toEndOf="@+id/textView23"
        android:layout_toRightOf="@+id/textView23"
        android:textColor="@color/colorPrimary"/>

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView33"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/tv_inicadasItemObserv"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:textColor="@color/colorPrimary"/>
</RelativeLayout>